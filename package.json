{
  "name": "rune-ui",
  "version": "0.1.0",
  "description": "An app for organizing your magic cards",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install",
    "start": "tsc && node-sass styles/rune.scss styles.css && concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run sass:w\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typings": "typings",
    "lite": "lite-server",
    "sass:w": "node-sass -w styles.scss styles.css",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "author": "Sam Noedel <sam.noedel@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.6",
    "@angular/compiler": "^2.0.0-rc.6",
    "@angular/core": "^2.0.0-rc.6",
    "@angular/platform-browser": "^2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.6",
    "core-js": "^2.4.1",
    "flux": "^2.1.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "^0.19.38",
    "zone.js": "^0.6.20"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "node-sass": "^3.9.3",
    "typescript": "^1.8.10",
    "typings": "^1.3.3"
  }
}
