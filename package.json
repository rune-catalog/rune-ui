{
  "name": "rune-ui",
  "version": "0.1.0",
  "description": "An app for organizing your magic cards",
  "main": "index.js",
  "repository": "rune-catalog/rune-ui",
  "scripts": {
    "start": "tsc && node-sass styles/rune.scss styles.css && concurrently \"npm run tsc:w\" \"npm run bs\" \"npm run sass:w\"",
    "prepublish": "npm run ngc && npm run rollup",
    "bs": "browser-sync start -c bs-config.json",
    "sass:w": "node-sass -w styles/rune.scss styles.css",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup.js"
  },
  "author": "Sam Noedel <sam.noedel@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@angular/common": "^2.1.2",
    "@angular/compiler": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/platform-browser": "^2.1.2",
    "@angular/platform-browser-dynamic": "^2.0.1",
    "@angular/router": "^3.1.2",
    "core-js": "^2.4.1",
    "flux-lite": "^2.1.0",
    "ramda": "^0.22.1",
    "reflect-metadata": "^0.1.8",
    "rune-ui-resources": "0.0.4",
    "rxjs": "^5.0.0-beta.12",
    "systemjs": "^0.19.38",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.1.2",
    "@angular/platform-server": "^2.0.1",
    "@types/core-js": "^0.9.34",
    "@types/ramda": "0.0.2",
    "browser-sync": "^2.17.5",
    "concurrently": "^3.1.0",
    "node-sass": "^3.10.0",
    "rollup": "^0.36.0",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "typescript": "^2.0.3"
  }
}
